<% if (!user) { %>
	<p>Welcome! Please <a href="/register">Register</a> or <a href="/login">Login</a>.</p>
<% } else { %>
	<p>Hello, <%= user.name %>. View your <a href="/profile">profile</a>.</p>
<% } %>
Users:
<ul>
	<% for (const user of users) { %>
		<li><a href="/profile/<%= user.name %>"><%= user.name %></a></li>
	<% } %>
</ul>
